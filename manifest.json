{
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "description": "Replace new tab page with bing photos.",
  "manifest_version": 2,
  "name": "newtab",
  "offline_enabled": true,
  "permissions": [ "alarms", "unlimitedStorage", "*://www.bing.com/*" ],
  "short_name": "newtab",
  "version": "0.0.0.6"
}
